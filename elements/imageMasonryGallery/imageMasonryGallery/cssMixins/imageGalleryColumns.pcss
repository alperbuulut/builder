@media (min-width: 544px) {
  .vce-image-masonry-gallery {
    &--columns-$selector {
      .vce-image-masonry-gallery-column {
        @if $columns != false {
          -webkit-flex: 0 0 calc(100% / $columns);
              -ms-flex: 0 0 calc(100% / $columns);
                  flex: 0 0 calc(100% / $columns);
          max-width: calc(100% / $columns);
        }
      }
    }
  }
}


